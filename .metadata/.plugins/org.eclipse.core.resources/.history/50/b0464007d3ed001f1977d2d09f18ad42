package br.com.hcm.ApiRestControleDeContatos.example;

public class ContatoSwaggerExample {
	
	public static final String POST_CONTATO_EXAMPLE = "{"
            + "\"tipoContato\": \"CELULAR\", "
            + "\"contato\": \"11 9865-8220\", "
            + "\"pessoa\": {\"id\": 1}"
            + "}";
	
	public static final String POST_RESPONSE_CONTATO_EXAMPLE = "{"
            + "\"id\": 1, "
            + "\"tipoContato\": \"CELULAR\", "
            + "\"contato\": \"11 9865-8220\", "
            + "\"pessoa\": {\"id\": 1, "
            + "\"nome\": \"Henrique\", "
            + "\"endereco\": \"Rua das Flores, 555\", "
            + "\"cep\": \"05050-555\", "
            + "\"cidade\": \"Osasco\", "
            + "\"uf\": \"SP\""
            + "}"
            + "}";
	
}
